		<constructor-arg  value="12"></constructor-arg>
	</bean> -->

	<bean class="com.spring.core.fundamental.constructorInjection.ConstructorInjectionModel" name="user1">
		<constructor-arg  value="OM PandeY"></constructor-arg>
		<constructor-arg  value="12"></constructor-arg>
		<constructor-arg  value="7464067216"></constructor-arg>
		<constructor-arg >
			<list>
				<ref bean="skill1" />
				<ref bean="skill2" />
				<ref bean="skill3" />
				<ref bean="skill4" />
				<ref bean="skill5" />
			</list>
		</constructor-arg>
		<constructor-arg  ref="address1"></constructor-arg>
	</bean>

